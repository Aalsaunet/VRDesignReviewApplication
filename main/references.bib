issn = {18735339},
journal = {Advances in Engineering Software},
keywords = {Computer aided design,Ship design,Shipbuilding,Software,Technology,Virtual Reality},
number = {C},
pages = {30--40},
title = {{Virtual Reality in a shipbuilding environment}},
volume = {81},
year = {2015}
}

