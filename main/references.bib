@article{Draper2001,
abstract = {Novel patterns of visual-vestibular intersensory stimulation often result in symptoms of simulator sickness, raising health and safety concerns regarding virtual environment exposure. Two experiments investigated the effect of conflicting visual-vestibular cues on subjective reports of simulator sickness during and after a 50-min exposure to a head-coupled virtual interface. Virtual image scale factors (0.5. 1.0, 2.0 magnification, generated by varying geometric field of view angle) were investigated in Experiment 1, and additional system time delays (125, 250 ms) were investigated in Experiment 2. Simulator sickness metrics included spoken self-reports during exposure and simulator sickness questionnaires (pre-exposure, immediate postexposure, and 20 min postexposure). Head yaw angular position data were also recorded. Reports of simulator sickness symptoms were significantly greater in the minification (0.5) and magnification (2.0) image scale factor conditions than in the neutral condition (1.0). Simulator sickness did not vary with changes in time delay, however. Furthermore, a comparison across experiments suggests no appreciable increase in simulator sickness with increasing time delays above the nominal value (48 ms). Head angular position data exhibited certain systematic variations across conditions. Actual or potential applications of this research include virtual environment training, simulation, and entertainment systems.},
author = {Draper, M H and Viire, E S and Furness, T a and Gawron, V J},
doi = {10.1518/001872001775992552},
issn = {00187208},
journal = {Human factors},
number = {1},
pages = {129--146},
pmid = {11474759},
title = {{Effects of image scale and system time delay on simulator sickness within head-coupled virtual environments.}},
volume = {43},
year = {2001}
}


@article{Lin2004,
abstract = {This study developed a new procedure, a Virtual Guiding Avatar (VGA), which combined self-motion prediction cues and an independent visual background (IVB) to alleviate simulator sickness (SS). The VGA, which was embodied as an abstract airplane, was designed to lead the participant along a horizontal motion trajectory through a virtual environment. Both motion prediction cues and IVBs, which provide an earth-fixed reference frame, reduced SS in separate previous studies. Participants were exposed to complex visual motion through a cartoon-like simulated environment in a very wide field of view driving simulator. Participants' responses to avatars with varying motion properties - fixed, rotation only or rotation plus translation - were assessed using a within-subjects experimental design. Results indicated that SS was reduced by a VGA that presented rotational cues alone or rotation plus translation. The VGA also increased participants' sense of presence and enjoyment relative to conditions lacking a VGA. The VGA procedure can be used to enhance user experiences in immersive virtual environments as well as to improve motion simulator design.},
author = {Lin, J J W and Abi-Rached, H and Lahav, M},
doi = {10.1145/985692.985783},
file = {:D$\backslash$:/Dropbox/Privat/Master/Reference documents/Virtual Guiding Avatar - An Effective Procedure to Reduce.pdf:pdf},
isbn = {1581137028},
journal = {Conference on Human Factors in Computing Systems - Proceedings},
keywords = {Avatar; Motion prediction; Prediction; Simulator,Control systems; Feedback; Human computer interact,Motion estimation},
mendeley-groups = {Most Relevant},
number = {1},
pages = {719--726},
title = {{Virtual guiding avatar: An effective procedure to reduce simulator sickness in virtual environments}},
url = {http://www.scopus.com/inward/record.url?eid=2-s2.0-4544250250{\&}partnerID=40{\&}md5=e8702da495f6ad516a11210168b1efd0},
volume = {6},
year = {2004}
}


@article{Stanney2003,
abstract = {For those interested in using head-coupled PC-based immersive virtual$\backslash$nenvironment (VE) technology to train, entertain, or inform, it is$\backslash$nessential to understand the effects this technology has on its users.$\backslash$nThis study investigated potential adverse effects, including the$\backslash$nsickness associated with exposure and extreme responses (emesis,$\backslash$nflashbacks). Participants were exposed to a VE for 15 to 60 min, with$\backslash$neither complete or streamlined navigational control and simple or$\backslash$ncomplex scenes, after which time measures of sickness were obtained.$\backslash$nMore than 80{\%} of participants experienced nausea, oculomotor$\backslash$ndisturbances, and/or disorientation, with disorientation potentially$\backslash$nlasting {\textgreater}24 hr. Of the participants, 12.9{\%} prematurely ended their$\backslash$nexposure because of adverse effects; of these, 9.2{\%} experienced an$\backslash$nemetic response, whereas only 1.2{\%} of all participants experienced$\backslash$nemesis. The results indicate that designers may be able to reduce these$\backslash$nrates by limiting exposure duration and reducing the degrees of freedom$\backslash$nof the user's navigational control. Results from gender, body mass, and$\backslash$npast experience comparisons indicated it may be possible to identify$\backslash$nthose who will experience adverse effects attributable to exposure and$\backslash$nwarn such individuals. Applications for this research include military,$\backslash$nentertainment, and any other interactive systems for which designers$\backslash$nseek to avoid adverse effects associated with exposure.},
